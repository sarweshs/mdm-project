# MDM Project Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key (Required for default LLM provider)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Key (Optional, for alternative LLM provider)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration (Optional, defaults are used in docker-compose.yml)
POSTGRES_DB=mdm_db
POSTGRES_USER=mdmuser
POSTGRES_PASSWORD=mdm_password 